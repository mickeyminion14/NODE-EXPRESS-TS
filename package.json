    "swagger-ui-express": "^4.1.6",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.42",
    "@types/swagger-ui-express": "^4.1.3",
